    <script>

        //2) desenvolva um programa que leia 10 números e indique se o número é par ou impar.

        var numero;
        var i= 0;

        do{
            numero= prompt("Digite o numero");

            if(numero % 2 == 0){

                alert("O numero   "  + numero + " É PAR");

            }else{

                alert("O numero "  + numero  + " É IMPAR");
            }

            i++

        }while( i<=10);
        
    
    
    
    
    
    
    </script>
